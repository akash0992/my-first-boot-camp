		function change()
		{
			document.getElementById("d1").style.color="red";
		}


		function rechange()
		{
			document.getElementById("d1").style.color="yellow";
		}

		function change2()
		{
			document.getElementById("d2").style.color="yellow";
		}


		function rechange2()
		{
			document.getElementById("d2").style.color="red";
		}
